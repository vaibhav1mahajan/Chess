datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output = "../generated/client"
}

model User {
  id           String       @id   @default(uuid())
  username     String       @unique
  password     String
  profilePic   String?
  createdAt    DateTime     @default(now())
  updatedAt    DateTime     @default(now())
}